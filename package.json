{
  "version": "0.0.1",
  "description": "A new NPM package",
  "keywords": [
    "node",
    "typescript",
    "eslint",
    "prettier",
    "vitest"
  ],
  "license": "MIT",
  "author": {
    "name": "Drew Lytle",
    "email": "contact@drewis.cool",
    "url": "https://drewis.cool"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "cross-env NODE_ENV=development nodemon -w src -x ts-node --files -H -T ./src/index.ts",
    "start": "node ./dist/index.js",
    "deploy:prod": "node ./dist/commands/deploy-commands.js",
    "deploy": "cross-env NODE_ENV=development ts-node ./src/commands/deploy-commands.ts",
    "lint": "eslint --ext .js,.ts .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "test": "vitest"
  },
  "dependencies": {
    "@napi-rs/canvas": "^0.1.41",
    "discord.js": "^14.11.0",
    "dotenv": "^16.1.4",
    "typescript": "^4.8.4",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/jest-image-snapshot": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "jest-image-snapshot": "^6.1.0",
    "msw": "^1.2.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "vite": "^3.2.3",
    "vite-tsconfig-paths": "^3.5.2",
    "vitest": "^0.25.2"
  },
  "engines": {
    "node": ">=14"
  }
}
